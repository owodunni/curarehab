{
  "ci": {
    "collect": {
      "url": [
        "http://localhost:4173",
        "http://localhost:4173/en",
        "http://localhost:4173/blog",
        "http://localhost:4173/blog/vulvodyni"
      ],
      "startServerCommand": "npm run preview"
    },
    "assert": {
      "preset": "lighthouse:recommended"
    },
    "upload": {
      "target": "filesystem",
      "outputDir": "./build/lighthouse",
      "reportFilenamePattern": "%%PATHNAME%%.report.%%EXTENSION%%"
    }
  }
}
